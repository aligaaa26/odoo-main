<odoo>
    <record id="group_guru_sekolah" model="res.groups">
        <field name="name">Guru Sekolah</field>
        <field name="category_id" ref="base.module_category_hidden"/>
    </record>

    <record id="access_guru_sekolah" model="ir.model.access">
        <field name="name">access_guru_sekolah</field>
        <field name="model_id" ref="model_sekolah"/>
        <field name="group_id" ref="group_guru_sekolah"/>
        <field name="perm_create">True</field>
        <field name="perm_write">False</field>
        <field name="perm_read">True</field>
        <field name="perm_unlink">False</field>
    </record>
</odoo>